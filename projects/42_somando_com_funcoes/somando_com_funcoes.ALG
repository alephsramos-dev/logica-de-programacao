algoritmo "semnome"
var
    V1, V2, S: Inteiro
Funcao Soma(A, B: Inteiro) : Inteiro
inicio
   Retorne A + B
FimFuncao
inicio
    Escreva("Digite o primeiro valor: ")
    Leia(V1)
    Escreva("Digite o segundo valor: ")
    Leia(V2)
    S <- Soma(V1, V2)
    Escreval("Escreva a soma:", S)
fimalgoritmo